{
  "name": "maxwellimpact/laravel-password-reset",
  "description": "Laravel Password Reset Extension to enable Injected Repositories",
  "license": "MIT",
  "authors": [
    {
      "name": "Jonathan Cox",
      "email": "joncox@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "laravel/framework": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0",
    "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
  },
  "autoload": {
    "psr-4": {
      "Maxwellimpact\\PasswordReset\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maxwellimpact\\PasswordReset\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maxwellimpact\\PasswordReset\\PasswordResetServiceProvider"
      ]
    }
  }
}
